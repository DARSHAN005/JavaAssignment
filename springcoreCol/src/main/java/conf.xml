<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/bcontext"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">
 
 
 <bean class="org.capge.springcoreCol.Emp" name="emp">
 		<property name="name" value="Sachin"/>
 		
 		<property name="phones">
 			<list>
 				<value>samsung</value>
 				<value>one plus</value>
 				<value>nokia</value>
 			</list>
 		</property>
 			<property name="address">
 			<set>
 				<value>bangalore</value>
 				<value>hyderbad</value>
 				<value>chennai</value>
 			</set>
 		</property>
 			<property name="courses">
 			<map>
 				<entry key="Java" value="1-month"/>
 				<entry key="python" value="2-month"/>
 				<entry key="C" value="1-month"/>
 				<entry key="C++" value="2-month"/>
 			</map>
 		</property>
 </bean>
</beans>